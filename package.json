{
  "displayName": "Homebridge Arlo",
  "name": "homebridge-arlo",
  "version": "2.1.0",
  "description": "Arlo plugin for homebridge",
  "author": "homebridge-plugins",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts",
    "snyk test": "snyk test"
  },
  "license": "ISC",
  "keywords": [
    "homebridge-plugin",
    "arlo",
    "homebridge"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/donavanbecker/homebridge-arlo.git"
  },
  "bugs": {
    "url": "http://github.com/donavanbecker/homebridge-arlo/issues"
  },
  "engines": {
    "homebridge": "^1.4.0",
    "node": "^14.18.3 || ^16.13.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "homebridge": "^1.4.0",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "typescript-axios-wb": "^1.0.3"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "@koush/arlo": "^1.2.4",
    "axios": "^0.25.0",
    "node-arlo-cameras": "^1.3.5",
    "rxjs": "^7.5.2"
  }
}
