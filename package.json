{
  "displayName": "Homebridge Arlo",
  "name": "homebridge-arlo",
  "version": "2.1.0",
  "description": "Arlo plugin for homebridge",
  "author": "homebridge-plugins",
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts",
    "snyk test": "snyk test"
  },
  "license": "ISC",
  "keywords": [
    "homebridge-plugin",
    "arlo",
    "homebridge"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/donavanbecker/homebridge-arlo.git"
  },
  "bugs": {
    "url": "http://github.com/donavanbecker/homebridge-arlo/issues"
  },
  "engines": {
    "node": ">=12.18.3",
    "homebridge": ">=1.2.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@types/debug": "^4.1.7",
    "@types/ip": ">=1.1.0",
    "@types/node": "^14.18.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "homebridge": "^1.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "snyk": "^1.820.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "axios-cookiejar-support": "^2.0.3",
    "debug": "^4.3.3",
    "events": "^3.3.0",
    "eventsource": "^1.1.0",
    "ffmpeg-for-homebridge": "^0.0.9",
    "ip": ">=1.1.5",
    "moment": "^2.29.1",
    "node-forge": "^1.2.0",
    "node-html-parser": "^5.2.0",
    "path": "^0.12.7",
    "tough-cookie": "^4.0.0",
    "uuid": "8.3.2",
    "wrap-text": "^1.0.9"
