  },
  "engines": {
    "homebridge": "^1.4.0",
    "node": "^14.18.3 || ^16.13.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@types/debug": "^4.1.7",
    "@types/imap": "^0.8.35",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
